; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Scoreboard"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/image="res://assets/icons/app_icons/app_icon_256px.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://assets/icons/app_icons/app_icon_128px.png"
config/windows_native_icon="res://assets/icons/platform_icons/icon_windows.ico"

[autoload]

Env="*res://scripts/env.gd"
IO="*res://scripts/globals/io.gd"

[display]

window/size/viewport_width=1024
window/size/viewport_height=640
display_server/driver.linuxbsd="wayland"

[gui]

theme/custom="res://resources/theme/default_theme.tres"

[rendering]

textures/vram_compression/import_etc2_astc=true
environment/defaults/default_clear_color=Color(0.0705882, 0.0705882, 0.0705882, 1)
